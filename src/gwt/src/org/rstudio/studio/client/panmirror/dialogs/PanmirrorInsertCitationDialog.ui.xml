<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
   xmlns:g="urn:import:com.google.gwt.user.client.ui"
   xmlns:rw="urn:import:org.rstudio.core.client.widget">
   <ui:with field="res" type="org.rstudio.studio.client.panmirror.dialogs.PanmirrorDialogsResources" />
   <ui:style>
   
      
   </ui:style>
   
   <g:VerticalPanel styleName="{res.styles.dialog}">
   
      <g:Label text="Citation ID (e.g. @Knuth):"></g:Label>
      <g:TextBox styleName="{res.styles.fullWidth} {res.styles.spaced}" ui:field="citationId_"></g:TextBox>
    
      <g:Label text="Locator (e.g. pp. 33-35):"></g:Label>
      <rw:TextBoxWithCue cueText="(Optional)" styleName="{res.styles.fullWidth} {res.styles.spaced}" ui:field="locator_"></rw:TextBoxWithCue>
      	
   </g:VerticalPanel>

</ui:UiBinder> 
